<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.27</Version>
    <Company>Rudenet</Company>
    <Authors>Proxytype</Authors>
    <AssemblyVersion>0.0.2.7</AssemblyVersion>
    <FileVersion>0.0.2.7</FileVersion>
    <Description>Simple HTTP/S files downloader, supporting multi parts connections</Description>
    <RepositoryType>Github</RepositoryType>
    <RepositoryUrl>https://github.com/proxytype/UrlDownloadLib</RepositoryUrl>
    <PackageTags>Http, Files, Downloads</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/proxytype/UrlDownloadLib/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

</Project>
